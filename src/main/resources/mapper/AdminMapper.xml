<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.krl.visiteducationbackend.mapper.AdminMapper">
    
    <resultMap id="AdminMap" type="cn.krl.visiteducationbackend.entity.Admin">
        <id column="id" property="id"></id>
        <result column="name" property="name"></result>
        <result column="password" property="password"></result>
        <result column="salt" property="salt"></result>
        <result column="gmtCreate" property="gmtCreate"></result>
        <result column="gmtCodified" property="gmtModified"></result>
    </resultMap>

    <select id="getByName" resultMap="AdminMap">
      select * from admin where #{name}=name
    </select>
</mapper>
